<div id="core">
  <div id="header">
    <a href="/" id="logo">Energy Transition Model (lite)</a>
  </div>

  <div id="scenes">
    <% for scene in @scenes: %>
      <div class="scene" id="scene-<%= scene.id %>">
        <a href="<%= scene.get 'href' %>" title="<%= scene.get 'name' %>">
          <%= scene.get 'name' %>
        </a>
      </div>
    <% end %>
  </div>
</div>

json.id          @scene.id
json.name        @scene.name
json.href        scene_path(@scene.id)

json.leftInputs  @scene.left_scene_inputs.map(&:remote_id)
json.rightInputs @scene.right_scene_inputs.map(&:remote_id)

json.inputs(@scene.scene_inputs) do |input|
  json.ingest! ( render partial: 'embeds/input', object: input )
end

json.centerVis   'supply-demand'
json.mainVis     %w( renewables co2-emissions costs )
